    <stall aoa="14" width="4" peak="1.5"/>
    <flap0 start="0" end=".67" lift="1.5" drag="1.7"/>
    <flap1 start=".688" end=".858" lift="1.3" drag="1.1"/>
    <slat start=".2" end=".8" aoa="2" drag="1.2"/>
    <spoiler start=".02" end=".3" lift="0.0" drag="5.0"/>
    <control-input axis="/controls/flight/flaps" control="FLAP0"/>
    <control-speed control="FLAP0" transition-time="9"/>
    <control-output control="FLAP0" prop="/surface-positions/flap-pos-norm"/>
