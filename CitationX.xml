  <fuselage ax="0.748" ay="-1.7" az="0.6" bx="-5.38" by="-1.7" bz="0.6"
  width="1.4" taper="0.7" midpoint="0.2"/><!--RH engine-->


  <wing x="1.120" y="0.90" z="-1.0" length="11" chord="4.2"
