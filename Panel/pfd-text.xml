<!--
Syd Adams
Primus 1000 PFD  messages
-->

<PropertyList>

    <name>PFD text</name>
    <w-base>200</w-base>
    <h-base>200</h-base>
    <w>200</w>
    <h>200</h>

    <layers>

    <layer>
            <name>VNV/FMS</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>4</point-size>
            <color>
                <red>0.9</red>
                <green>0.2</green>
                <blue>0.6</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>/instrumentation/primus1000/fms-mode</property>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>43</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>62</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <condition>
                    <property>instrumentation/primus1000/dc550/fms</property>
            </condition>
            <type>group</type>

            <layer>
                <name>NAV/SRC</name>
                <type>text</type>
                <font>arial_black</font>
                <point-size>4</point-size>
                <color>
                    <red>0.9</red>
                    <green>0.2</green>
                    <blue>0.6</blue>
                </color>
                <chunks>
                    <chunk>
                        <type>text-value</type>
                        <property>/instrumentation/primus1000/nav-string</property>
                    </chunk>
                </chunks>
                <transformations>
                    <transformation>
                        <type>x-shift</type>
                        <offset>42</offset>
                    </transformation>
                    <transformation>
                        <type>y-shift</type>
                        <offset>-22</offset>
                    </transformation>
                </transformations>
            </layer>

            <layer>
                <name>NAV/DST</name>
                <type>text</type>
                <font>arial_black</font>
                <point-size>4</point-size>
                <color>
                    <red>0.9</red>
                    <green>0.2</green>
                    <blue>0.6</blue>
                </color>
                <chunks>
                    <chunk>
                        <type>number-value</type>
                        <property>/instrumentation/primus1000/nav-dist-nm</property>
                        <format>%3.1f</format>
                    </chunk>
                </chunks>
                <transformations>
                    <transformation>
                        <type>x-shift</type>
                        <offset>-5</offset>
                    </transformation>
                    <transformation>
                        <type>y-shift</type>
                        <offset>-6</offset>
                    </transformation>
                </transformations>
            </layer>

            <layer>
                <name>NAVdst</name>
                <type>text</type>
                <font>arial_black</font>
                <point-size>4</point-size>
                <color>
                    <red>0.8</red>
                    <green>0.8</green>
                    <blue>0.8</blue>
                </color>
                <chunks>
                    <chunk>
                        <type>literal</type>
                        <text>NM</text>
                    </chunk>
                </chunks>
                <transformations>
                    <transformation>
                        <type>x-shift</type>
                        <offset>15</offset>
                    </transformation>
                    <transformation>
                        <type>y-shift</type>
                        <offset>0</offset>
                    </transformation>
                </transformations>
            </layer>

        </layer>

        <layer>
            <condition>
                    <not>
                        <property>instrumentation/primus1000/dc550/fms</property>
                    </not>
            </condition>
            <type>group</type>

            <layer>
                <name>NAV/SRC</name>
                <condition>
                    <equals>
                        <property>instrumentation/primus1000/dc550/nav</property>
                        <value>0</value>
                    </equals>
                </condition>
                <type>text</type>
                <font>arial_black</font>
                <point-size>4</point-size>
                <color>
                    <red>0.1</red>
                    <green>0.9</green>
                    <blue>0.4</blue>
                </color>
                <chunks>
                    <chunk>
                        <type>text-value</type>
                        <property>/instrumentation/primus1000/nav-string</property>
                    </chunk>
                </chunks>
                <transformations>
                    <transformation>
                        <type>x-shift</type>
                        <offset>42</offset>
                    </transformation>
                    <transformation>
                        <type>y-shift</type>
                        <offset>-22</offset>
                    </transformation>
                </transformations>
            </layer>

            <layer>
                <name>NAV/DST</name>
                <condition>
                    <equals>
                        <property>instrumentation/primus1000/dc550/nav</property>
                        <value>0</value>
                    </equals>
                </condition>
                <type>text</type>
                <font>arial_black</font>
                <point-size>4</point-size>
                <color>
                    <red>0.1</red>
                    <green>0.9</green>
                    <blue>0.4</blue>
                </color>
                <chunks>
                    <chunk>
                        <type>number-value</type>
                        <property>instrumentation/primus1000/nav-dist-nm</property>
                        <format>%3.1f</format>
                    </chunk>
                </chunks>
                <transformations>
                    <transformation>
                        <type>x-shift</type>
                        <offset>-5</offset>
                    </transformation>
                    <transformation>
                        <type>y-shift</type>
                        <offset>-6</offset>
                    </transformation>
                </transformations>
            </layer>

            <layer>
                <name>NAV/SRC2</name>
                <condition>
                    <equals>
                        <property>instrumentation/primus1000/dc550/nav</property>
                        <value>1</value>
                    </equals>
                </condition>
                <type>text</type>
                <font>arial_black</font>
                <point-size>4</point-size>
                <color>
                    <red>0.9</red>
                    <green>0.85</green>
                    <blue>0.1</blue>
                </color>
                <chunks>
                    <chunk>
                        <type>text-value</type>
                        <property>/instrumentation/primus1000/nav-string</property>
                    </chunk>
                </chunks>
                <transformations>
                    <transformation>
                        <type>x-shift</type>
                        <offset>42</offset>
                    </transformation>
                    <transformation>
                        <type>y-shift</type>
                        <offset>-22</offset>
                    </transformation>
                </transformations>
            </layer>

            <layer>
                <name>NAV/DST2</name>
                <condition>
                    <equals>
                        <property>instrumentation/primus1000/dc550/nav</property>
                        <value>1</value>
                    </equals>
                </condition>
                <type>text</type>
                <font>arial_black</font>
                <point-size>4</point-size>
                <color>
                    <red>0.9</red>
                    <green>0.85</green>
                    <blue>0.1</blue>
                </color>
                <chunks>
                    <chunk>
                        <type>number-value</type>
                        <property>/instrumentation/primus1000/nav-dist-nm</property>
                        <format>%3.1f</format>
                    </chunk>
                </chunks>
                <transformations>
                    <transformation>
                        <type>x-shift</type>
                        <offset>-5</offset>
                    </transformation>
                    <transformation>
                        <type>y-shift</type>
                        <offset>-6</offset>
                    </transformation>
                </transformations>
            </layer>

            <layer>
                <name>NAVdst</name>
                <type>text</type>
                <font>arial_black</font>
                <point-size>4</point-size>
                <color>
                    <red>0.8</red>
                    <green>0.8</green>
                    <blue>0.8</blue>
                </color>
                <chunks>
                    <chunk>
                        <type>literal</type>
                        <text>NM</text>
                    </chunk>
                </chunks>
                <transformations>
                    <transformation>
                        <type>x-shift</type>
                        <offset>15</offset>
                    </transformation>
                    <transformation>
                        <type>y-shift</type>
                        <offset>0</offset>
                    </transformation>
                </transformations>
            </layer>

        </layer>


        <layer>
            <name>Wind Speed</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>4.5</point-size>
            <color>
                <red>0.7</red>
                <green>0.7</green>
                <blue>0.3</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>environment/wind-speed-kt</property>
                    <format>%02.0f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-65</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-85</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <condition>
                    <equals>
                        <property>instrumentation/primus1000/dc550/timer</property>
                        <value>0</value>
                    </equals>
            </condition>
            <name>Ground Speed</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>4</point-size>
            <color>
                <red>0.1</red>
                <green>0.9</green>
                <blue>0.4</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>velocities/groundspeed-kt</property>
                    <format>% 3.0f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>42</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-98</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <condition>
                    <equals>
                        <property>instrumentation/primus1000/dc550/timer</property>
                        <value>2</value>
                    </equals>
            </condition>
            <name>ET</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>4</point-size>
            <color>
                <red>0.1</red>
                <green>0.9</green>
                <blue>0.4</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>instrumentation/clock/m877/ET-string</property>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>39</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-98</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <condition>
                    <equals>
                        <property>instrumentation/primus1000/dc550/timer</property>
                        <value>1</value>
                    </equals>
            </condition>
            <name>TTG</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>4</point-size>
            <color>
                <red>0.1</red>
                <green>0.9</green>
                <blue>0.4</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>instrumentation/primus1000/nav-time</property>
                    </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>39</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-98</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>INHG</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>5</point-size>
            <color>
                <red>0.39</red>
                <green>0.75</green>
                <blue>0.85</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>instrumentation/primus1000/baro-kpa</property>
                    </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>58</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-12</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>INHG mrkr</name>
            <condition>
                <not>
                    <property>instrumentation/primus1000/dc550/hpa</property>
                </not>
            </condition>
            <type>text</type>
            <font>arial_black</font>
            <point-size>4</point-size>
            <color>
                <red>0.8</red>
                <green>0.8</green>
                <blue>0.8</blue>
            </color>
            <chunks>
                <chunk>
                    <type>literal</type>
                    <text> IN</text>
                    </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>78</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-12</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>HPA mrkr</name>
            <condition>
                <property>instrumentation/primus1000/dc550/hpa</property>
            </condition>
            <type>text</type>
            <font>arial_black</font>
            <point-size>4</point-size>
            <color>
                <red>0.8</red>
                <green>0.8</green>
                <blue>0.8</blue>
            </color>
            <chunks>
                <chunk>
                    <type>literal</type>
                    <text>HPA</text>
                    </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>78</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-12</offset>
                </transformation>
            </transformations>
        </layer>

<!--    FD MODE ANNUNCIATORS-->

        <layer>
            <name>MINIMUMS</name>
            <condition>
                <property>instrumentation/primus1000/dc550/RA-alert</property>
            </condition>
            <type>text</type>
            <font>arial_black</font>
            <point-size>4.5</point-size>
            <color>
                <red>0.1</red>
                <green>0.9</green>
                <blue>0.3</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>instrumentation/mk-viii/inputs/arinc429/decision-height</property>
                    <format>%3.0f</format>
                </chunk>
                <color>
                    <red>0.8</red>
                    <green>0.8</green>
                    <blue>0.8</blue>
                </color>
                <chunk>
                    <type>literal</type>
                    <text> RA</text>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>10</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-10</offset>
                </transformation>
            </transformations>
        </layer>

<!-- AP Annunciators-->

        <layer>
            <name>AP annun</name>
            <condition>
                <not>
                    <property>autopilot/locks/passive-mode</property>
                </not>
            </condition>
            <type>text</type>
            <font>arial_black</font>
            <point-size>5</point-size>
            <color>
                <red>0.2</red>
                <green>0.8</green>
                <blue>0.3</blue>
            </color>
            <chunks>
                <chunk>
                    <type>literal</type>
                    <text>AP ENG</text>
                </chunk>
        </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-14</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>84</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>LAT mode</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>5</point-size>
            <color>
                <red>0.2</red>
                <green>0.8</green>
                <blue>0.3</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>instrumentation/flightdirector/LAT-annun</property>
                </chunk>
        </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-30</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>92</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>LAT arm mode</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>5</point-size>
            <color>
                <red>0.7</red>
                <green>0.7</green>
                <blue>0.7</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>instrumentation/flightdirector/LAT-arm-annun</property>
                </chunk>
        </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-50</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>92</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>VRT mode</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>5</point-size>
            <color>
                <red>0.2</red>
                <green>0.8</green>
                <blue>0.3</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>instrumentation/flightdirector/VRT-annun</property>
                </chunk>
        </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>10</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>92</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>VRT  sub mode</name>
            <type>text</type>
            <font>arial_black</font>
            <point-size>5</point-size>
            <color>
                <red>0.7</red>
                <green>0.7</green>
                <blue>0.7</blue>
            </color>
            <chunks>
                <chunk>
                    <type>text-value</type>
                    <property>instrumentation/flightdirector/VRT-arm-annun</property>
                </chunk>
        </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>30</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>92</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <type>group</type>

        <layer>
            <name>DTK mode</name>
            <condition>
                <property>instrumentation/primus1000/dc550/fms</property>
            </condition>
            <type>text</type>
            <font>arial_black</font>
            <point-size>5</point-size>
            <color>
                <red>0.8</red>
                <green>0.8</green>
                <blue>0.8</blue>
            </color>
            <chunks>
                <chunk>
                    <type>literal</type>
                    <text>DTK</text>
                </chunk>
        </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-90</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-26</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>DTK value</name>
            <condition>
                <property>instrumentation/primus1000/dc550/fms</property>
            </condition>
            <type>text</type>
            <font>arial_black</font>
            <point-size>5.5</point-size>
            <color>
                <red>0.1</red>
                <green>0.9</green>
                <blue>0.4</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>autopilot/settings/true-heading-deg</property>
                    <format>%3.0f</format>
                </chunk>
        </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>0</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-8</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>CRS1 mode</name>
            <condition>
                <not>
                    <property>instrumentation/primus1000/dc550/fms</property>
                </not>
            </condition>
            <type>text</type>
            <font>arial_black</font>
            <point-size>5</point-size>
            <color>
                <red>0.8</red>
                <green>0.8</green>
                <blue>0.8</blue>
            </color>
            <chunks>
                <chunk>
                    <type>literal</type>
                    <text>CRS</text>
                </chunk>
        </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-90</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-26</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>CRS1 value</name>
            <condition>
                <not>
                    <property>instrumentation/primus1000/dc550/fms</property>
                </not>
            </condition>
            <type>text</type>
            <font>arial_black</font>
            <point-size>5.5</point-size>
            <color>
                <red>0.1</red>
                <green>0.9</green>
                <blue>0.4</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
