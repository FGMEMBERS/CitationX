    <animation>
        <type>select</type>
        <object-name>Page</object-name>
        <object-name>L0</object-name>
        <object-name>L1</object-name>
        <object-name>L2</object-name>
        <object-name>L3</object-name>
        <object-name>L4</object-name>
        <object-name>L5</object-name>
        <object-name>L6</object-name>
        <object-name>R0</object-name>
        <object-name>R1</object-name>
        <object-name>R2</object-name>
        <object-name>R3</object-name>
        <object-name>R4</object-name>
        <object-name>R5</object-name>
        <object-name>R6</object-name>
        <object-name>S0</object-name>
        <object-name>CDU-lamp</object-name>
        <object-name>CDU-screen</object-name>
				<condition>
					<and>				
            <greater-than>
                <property>systems/electrical/outputs/efis</property>
                <value>15</value>
            </greater-than>
            <greater-than>
                <property>controls/lighting/cdu1</property>
                <value>0.02</value>
            </greater-than>
            <equals>
                <property>controls/electric/avionics-switch</property>
                <value>2</value>
            </equals>
					</and>					
        </condition>
    </animation>

