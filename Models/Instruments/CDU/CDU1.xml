                <property>controls/lighting/cdu1</property>
                <step>0.05</step>
                <min>0</min>
                <max>1.0</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>CDU-knob</object-name>
