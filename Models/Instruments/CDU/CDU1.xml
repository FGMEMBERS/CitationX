<!-- Hotspots -->

<animation>
	<type>pick</type>
	<object-name>btn.next</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>cdu.next();</script>
		</binding>
	</action>
</animation>

<animation>
	<type>pick</type>
	<object-name>btn.prev</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>cdu.previous();</script>
		</binding>
	</action>
</animation>

<animation>
	<type>pick</type>
	<object-name>btn.fpl</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>
		<binding>
			<condition>
				<or>
					<equals>
						<property>instrumentation/cdu/display</property>
						<value>NAVIDENT[0]</value>
					</equals>
					<equals>
						<property>instrumentation/cdu/display</property>
						<value>POS INIT[0]</value>
					</equals>
				</or>
			</condition>
			<command>nasal</command>
			<script>cdu.key('FPL');</script>
		</binding>
		<binding>
			<condition>
				<not>
					<property>instrumentation/cdu/display</property>
					<value>NAVIDENT[0]</value>
				</not>
			</condition>
			<command>nasal</command>
			<script>cdu.next();</script>
		</binding>

	</action>
</animation>

