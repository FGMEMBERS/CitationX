<!--  Citation X - Canvas CDU -->
<!--   C.Le Moigne (clm76) - 2017 -->


<PropertyList>

<path>CDU-screenL.ac</path>

    <animation>
        <type>select</type>
        <object-name>CDU.screenL</object-name>
        <condition>
          <and>
            <property>controls/electric/avionics-power</property>
            <greater-than>
              <property>controls/lighting/cdu</property>
              <value>0.05</value>
            </greater-than>
          </and>
				</condition>
    </animation>

</PropertyList>
