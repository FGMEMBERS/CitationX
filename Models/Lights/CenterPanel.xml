<?xml version="1.0"?>

<PropertyList>

    <path>CenterPanel.ac</path> 

    <animation>
        <type>select</type>
        <object-name>CenterPanel.Cone</object-name>
        <condition>
						<or>
	           	<property>systems/electrical/left-emer-bus</property>
	           	<property>systems/electrical/left-bus-norm</property>
						</or>
        </condition>
    </animation>

   <animation>
			<type>light</type>
			<light-type>spot</light-type>
			<object-name>CenterPanel.Cone</object-name> 
			<nopreview/>
			<position>
				<x>-1.180</x>
				<y>0.0</y>
				<z>0.810</z>
			</position>
			<direction>
				<x>-0.1</x>
				<y>0</y>
				<z>-2</z>
			</direction>
			<ambient>
				<r>0.9</r>
				<g>0.7</g>
				<b>0.5</b>
				<a>1</a>
			</ambient>
			<diffuse>
				<r>0.7</r>
				<g>0.4</g>
				<b>0.3</b>
				<a>1</a>
			</diffuse>
			<specular>
				<r>0.7</r>
				<g>0.4</g>
				<b>0.3</b>
				<a>1</a>
			</specular>
			<dim-factor>
				<property>controls/lighting/ctr-lights</property>
			</dim-factor>
			<attenuation>
				<c>0.0</c>
				<l>3</l>
				<q>50</q>
			</attenuation>
			<near-m>0.001</near-m>
			<far-m>2</far-m>
			<cutoff>70</cutoff>
    </animation>
 </PropertyList>
