<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

<!-- Flood -->

  <filter>
      <name>Flood average</name>
      <type>gain</type>
      <gain>1.0</gain>
      <input>
          <expression>
              <table>
                  <property>controls/lighting/flood</property> 
                  <entry><ind>0</ind><dep>0.1</dep></entry>
                  <entry><ind>1</ind><dep>0.5</dep></entry>
              </table>
          </expression>
      </input>
      <output>controls/lighting/flood-norm</output>
	</filter>

<!-- Center Panel Light -->

  <model>
    <name>CenterPanel</name>
    <path>Models/Lights/CenterPanel.xml</path>
    <offsets>
      <x-m>-6.613</x-m>
      <y-m>0</y-m>
      <z-m>-0.590</z-m>
    </offsets>
  </model>

</PropertyList>
